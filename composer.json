{
    "name": "medspec/laravel5-magento",
    "description": "A simple Magento Integration using SOAP v1 and v2. Forked from michaelkmartin/laravel5-magento",
    "version": "1.0.2",
    "keywords": ["laravel", "magento", "soap"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Kyle Martin",
            "email": "hello@michaelkmartin.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/contracts": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*"
    },
    "require-dev": {
        "guzzle/guzzle": "^3.7",
        "guzzlehttp/guzzle" : "^5.0|^6.0",
        "kriswallsmith/buzz": "^0.10",
        "orchestra/testbench": "~3.0",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^4.8|^5.0"
    },
    "autoload": {
        "psr-4": {
            "MichaelMartin\\Magento\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
